
@{
    ViewData["Title"] = "UploadJson";
}

<h1>UploadJson</h1>

<form class="col-xs-12" method="post" action="/Stock/UploadJson" enctype="multipart/form-data">

    <div class="form-group">
        <input type="file" class="form-control" name="jsonFile" />
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary col-xs-12">Upload</button>
    </div>
</form>

